{
  "name": "blc-bot",
  "version": "0.0.1",
  "main": "client.js",
  "repository": "https://github.com/bpas247/OLCBot.git",
  "author": "Brady Pascoe <bpas247@hotmail.com>",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "discord.js": "^11.4.2",
    "dotenv": "^6.1.0",
    "pg-promise": "^8.5.1"
  },
  "scripts": {
    "test": "jest",
    "build": "babel src --out-dir lib --copy-files production/package.json production/Procfile production/yarn.lock",
    "start": "babel-node src/client.js"
  },
  "babel": {
    "plugins": [
      "@babel/plugin-transform-modules-commonjs"
    ]
  },
  "jest": {
    "modulePathIgnorePatterns": [
      "lib/*"
    ]
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/node": "^7.0.0",
    "@babel/plugin-transform-modules-commonjs": "^7.1.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "coveralls": "^3.0.2",
    "husky": "^1.1.2",
    "jest": "^23.6.0",
    "lint-staged": "^7.3.0",
    "prettier": "^1.14.3"
  }
}
